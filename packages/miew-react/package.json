{
  "name": "miew-react",
  "version": "0.0.1",
  "description": "3D Molecular Viewer",
  "license": "MIT",
  "homepage": "http://lifescience.opensource.epam.com/miew",
  "author": {
    "name": "EPAM Life Sciences",
    "url": "http://lifescience.opensource.epam.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epam/miew.git",
    "directory": "packages/miew-react"
  },
  "keywords": [
    "molecule",
    "visualizer",
    "chemistry"
  ],
  "bugs": {
    "url": "https://github.com/epam/miew/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "types": "dist/index.d.ts",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c -m true",
    "start": "cross-env NODE_ENV=development rollup -c -m true -w",
    "test": "run-s prettier stylelint test:lint test:unit",
    "test:lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "test:unit": "jest",
    "test:unit:update": "jest -u",
    "prettier": "prettier --check 'src/**/*.{js,jsx,json,ts,tsx}'",
    "prettier:write": "prettier --write 'src/**/*.{js,jsx,json,ts,tsx}'",
    "stylelint": "stylelint 'src/**/*.{css,less,scss}' --formatter verbose",
    "stylelint:fix": "stylelint 'src/**/*.{css,less,scss}' --formatter verbose --fix"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@reduxjs/toolkit": "^1.7.1",
    "clsx": "^1.1.1",
    "miew": "workspace:*",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "use-resize-observer": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-strip": "^2.0.0",
    "@svgr/rollup": "^5.4.0",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^27.4.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.4.1",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.1",
    "jest": "^27.4.5",
    "jest-canvas-mock": "^2.3.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-scss": "^4.0.2",
    "prettier": "^2.5.1",
    "rollup": "^2.60.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "sass": "^1.45.0",
    "stylelint": "13.13.1",
    "ts-jest": "26.5.6",
    "typescript": "^4.5.2"
  },
  "files": [
    "dist"
  ]
