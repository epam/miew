{
  "name": "miew",
  "version": "0.9.0",
  "description": "Miew - 3D Molecular Viewer",
  "author": "EPAM Systems, Inc.",
  "main": "dist/miew.js",
  "module": "dist/miew.module.js",
  "style": "dist/miew.min.css",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epam/miew.git",
    "directory": "packages/miew"
  },
  "license": "MIT",
  "scripts": {
    "ci": "run-s clean test-cover build",
    "eslint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "eslint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "prettier": "prettier --check \"./**/*.{js,jsx,json,ts,tsx}\"",
    "prettier:write": "prettier --write \"./**/*.{js,jsx,json,ts,tsx}\"",
    "stylelint": "stylelint \"./**/*.{css,less,scss}\" --formatter verbose",
    "stylelint:fix": "stylelint \"./**/*.{css,less,scss}\" --formatter verbose --fix",
    "lint": "run-s 'lint:*'",
    "lint-fix": "run-s 'lint:* --fix'",
    "lint:js": "eslint -f unix .*.js *.js src/ demo/ examples/ test/ tools/",
    "lint:css": "stylelint src/**/*.scss demo/**/*.scss",
    "test": "run-s prettier eslint test:unit",
    "test-cover": "cross-env NODE_ENV=coverage nyc mocha",
    "test:unit": "mocha",
    "test:e2e": "rimraf './test/e2e/mismatch/*.png' './test/e2e/mismatch/report-*.html' && mocha --config ./test/.e2e.mocharc.js",
    "clean": "rimraf 'build/*' 'coverage/*'",
    "build": "rollup -c -m true",
    "show:cover": "node tools/open.js coverage/lcov-report/index.html",
    "show:e2e": "node tools/open.js 'test/e2e/mismatch/*.html'",
    "preversion": "run-s lint test",
    "version": "cross-env NODE_ENV=production run-s build:js build:css dist",
    "jison": "node tools/jison.js src/utils/*.jison",
    "jison:one": "node ./tools/jison.js"
  },
  "bugs": {
    "url": "https://github.com/epam/miew/issues"
  },
  "homepage": "https://github.com/epam/miew#readme",
  "keywords": [
    "molecule",
    "protein",
    "chemistry",
    "webgl",
    "threejs"
  ],
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions",
    "IE >= 11",
    "last 2 iOS versions",
    "last 1 Android version",
    "last 2 ChromeAndroid versions",
    "last 2 FirefoxAndroid versions"
  ],
  "nyc": {
    "cache": true,
    "all": true,
    "include": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "exclude": [
      "@(node_modules|dist|build|vendor|demo|coverage|docs|examples|test|tools)",
      "**/*.test.js"
    ],
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "three": "0.131.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/register": "^7.8.6",
    "@babel/runtime": "^7.9.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-strip": "^2.1.0",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^6.0.0",
    "browserify": "^16.5.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "dirty-chai": "^2.0.1",
    "eslint": "^8.4.1",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "glob": "^7.1.6",
    "handlebars": "^4.7.5",
    "html-webpack-plugin": "^4.3.0",
    "jison": "^0.4.18",
    "mini-css-extract-plugin": "^0.9.0",
    "mkdirp": "^1.0.3",
    "mocha": "^7.1.2",
    "ncp": "^2.0.0",
    "node-resemble-js": "^0.2.0",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.3",
    "nyc": "^15.0.0",
    "open": "^7.0.3",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-calc": "^7.0.2",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-scss": "^2.0.0",
    "prettier": "^2.5.1",
    "proxyquire": "^2.1.3",
    "raw-loader": "^4.0.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "serve": "^13.0.2",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "spin.js": "^4.1.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.0.0",
    "toastr": "^2.1.4",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "yargs": "^15.3.0"
  }
}
