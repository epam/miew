{
  "name": "miew-app",
  "version": "0.0.1",
  "description": "React application demonstrating capabilities of Miew library",
  "author": "EPAM Systems, Inc.",
  "private": true,
  "main": "dist/main.js",
  "license": "MIT",
  "scripts": {
    "ci": "run-s clean build",
    "clean": "rimraf 'build/*'",
    "lint": "eslint -f unix --ext .js,.jsx src/ ",
    "lint:css": "stylelint '**/*.scss'",
    "build": "webpack --mode production",
    "start": "webpack-dev-server --mode development --open",
    "test": "run-s prettier eslint test:unit",
    "test:unit": "jest",
    "eslint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "eslint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "prettier": "prettier --check \"./**/*.{js,jsx,json,ts,tsx}\"",
    "prettier:write": "prettier --write \"./**/*.{js,jsx,json,ts,tsx}\"",
    "stylelint": "stylelint \"./**/*.{css,less,scss}\" --formatter verbose",
    "stylelint:fix": "stylelint \"./**/*.{css,less,scss}\" --formatter verbose --fix",
    "test-cover": "jest --coverage",
    "show:cover": "node tools/open.js coverage/lcov-report/index.html"
  },
  "keywords": [
    "molecule",
    "protein",
    "chemistry",
    "miew",
    "webgl",
    "threejs",
    "react"
  ],
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions",
    "IE >= 11",
    "last 2 iOS versions",
    "last 1 Android version",
    "last 2 ChromeAndroid versions",
    "last 2 FirefoxAndroid versions"
  ],
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "css": {
    "includePaths": [
      "./node_modules"
    ]
  },
  "dependencies": {
    "bootstrap": "=4.5.0",
    "bootstrap-icons": "=1.7.2",
    "bootstrap-switch-button-react": "=1.2.0",
    "classnames": "^2.2.6",
    "jquery": "=3.6.0",
    "jquery.terminal": "=2.20.1",
    "lodash": "^4.17.20",
    "miew": "0.9.0",
    "miew-react": "workspace:*",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-redux": "^7.2.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.6",
    "@testing-library/react": "^11.2.0",
    "@types/jest": "^25.2.2",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "eslint": "^8.4.1",
    "html-loader": "^1.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "jest-canvas-mock": "^2.2.0",
    "jest-simple-dot-reporter": "^1.0.5",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "prettier": "^2.5.1",
    "redux": "^4.0.5",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.0.0",
    "three": "0.131.3",
    "typescript": "^4.5.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.0"
  }
}
