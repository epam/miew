{
  "presets": ["@babel/env", ["@babel/react", {"runtime": "automatic"}], "@babel/typescript"],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-runtime",
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ],
  "env": {
    "coverage": {
      "plugins": [
        "istanbul"
      ]
    }
  }
}
