{
  "name": "miew-monorepo",
  "version": "0.11.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "npm": ">=9.0.0 <12.0.0",
    "node": ">=20.0.0 <25.0.0"
  },
  "scripts": {
    "clean": "rimraf coverage && yarn workspaces foreach -ptv run clean",
    "merge-cover": "mkdirp coverage && lcov-result-merger 'packages/*/coverage/lcov.info' 'coverage/lcov.info' --prepend-source-files --prepend-path-fix '..'",
    "ci": "yarn workspaces foreach -ptv run ci"
  },
  "devDependencies": {
    "lcov-result-merger": "^5.0.1",
    "mkdirp": "^3.0.1",
    "rimraf": "^6.0.1"
  },
  "packageManager": "yarn@3.6.3",
  "resolutions": {
    "jpeg-js": "0.4.4",
    "semver": "7.6.3",
    "underscore": "1.13.7",
    "express": "4.21.1",
    "form-data": "4.0.4"
  }
}
