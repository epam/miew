{
  "name": "miew",
  "version": "0.7.16",
  "description": "Miew - 3D Molecular Viewer",
  "author": "EPAM Systems, Inc.",
  "main": "dist/Miew.js",
  "module": "dist/Miew.module.js",
  "style": "dist/Miew.min.css",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epam/miew.git"
  },
  "license": "MIT",
  "scripts": {
    "ci": "run-s clean lint test-cover build",
    "lint": "run-s lint:*",
    "lint-fix": "run-s \"lint:* -- --fix\"",
    "lint:js": "eslint -f unix *.js src/ demo/ examples/ test/ tools/",
    "lint:css": "stylelint src/**/*.scss demo/**/*.scss",
    "test": "run-s test:unit",
    "test-cover": "cross-env NODE_ENV=coverage nyc mocha",
    "test:unit": "mocha",
    "test:e2e": "rimraf ./test/e2e/mismatch/*.png && mocha --opts ./test/mocha.e2e.opts",
    "start": "webpack-dev-server --mode development --open",
    "clean": "rimraf build/* coverage/*",
    "build": "run-s build:*",
    "build:js": "run-s build:js:*",
    "build:js:bundle": "rollup -c",
    "build:js:minify": "uglifyjs build/dist/Miew.js -o build/dist/Miew.min.js -mc drop_console=true --comments /copyright/i --source-map \"content='build/dist/Miew.js.map',url='Miew.min.js.map'\"",
    "build:css": "run-s build:css:*",
    "build:css:bundle": "node-sass --source-map=true --source-map-contents=true --output-style=expanded src/Miew.scss -o build/dist",
    "build:css:prefix": "postcss build/dist/Miew.css -o build/dist/Miew.css --map --use autoprefixer",
    "build:css:minify": "postcss build/dist/Miew.css -o build/dist/Miew.min.css --map --use cssnano",
    "build:demo": "webpack --mode production",
    "build:examples": "ncp examples/ build/examples/",
    "build:docs": "jsdoc -c jsdoc.config.js",
    "demo": "node tools/serve.js",
    "show:cover": "node tools/open.js coverage/lcov-report/index.html",
    "show:docs": "node tools/open.js build/docs/index.html",
    "show:e2e": "node tools/open.js test/e2e/mismatch/*.html",
    "dist": "ncp build/dist/ dist/ && git add -A dist",
    "preversion": "run-s lint test",
    "version": "cross-env NODE_ENV=production run-s build:js build:css dist",
    "jison": "node tools/jison.js src/utils/*.jison",
    "jison:one": "node ./tools/jison.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "bugs": {
    "url": "https://github.com/epam/miew/issues"
  },
  "homepage": "https://github.com/epam/miew#readme",
  "keywords": [
    "molecule",
    "protein",
    "chemistry",
    "webgl",
    "threejs"
  ],
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions",
    "IE >= 11",
    "last 2 iOS versions",
    "last 1 Android version",
    "last 2 ChromeAndroid versions",
    "last 2 FirefoxAndroid versions"
  ],
  "nyc": {
    "cache": true,
    "all": true,
    "include": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "exclude": [
      "@(node_modules|dist|vendor|demo|coverage|docs|examples|test|tools)",
      "**/*.test.js"
    ],
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "bootstrap": "^3.3.7",
    "bootstrap-switch": "^3.3.4",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "copy-webpack-plugin": "^4.5.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "v4.0.0-alpha.0",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "handlebars": "^4.0.11",
    "html-webpack-plugin": "^3.2.0",
    "jison": "^0.4.18",
    "jquery": "^3.3.1",
    "jquery.terminal": "^1.14.0",
    "jsdoc": "^3.5.5",
    "lodash": "^4.17.5",
    "mocha": "^5.1.0",
    "ncp": "^2.0.0",
    "node-resemble-js": "^0.2.0",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.6.0",
    "open": "^0.0.5",
    "postcss-cli": "^5.0.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-string": "^2.0.2",
    "sass-loader": "^7.0.1",
    "selenium-webdriver": "^3.5.0",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0",
    "spin.js": "^3.1.0",
    "string-replace-webpack-plugin": "^0.1.3",
    "style-loader": "^0.20.3",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "toastr": "^2.1.4",
    "uglify-js": "^3.3.21",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "yargs": "^11.0.0"
  }
}
