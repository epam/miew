{
  "name": "miew",
  "version": "0.8.4",
  "description": "Miew - 3D Molecular Viewer",
  "author": "EPAM Systems, Inc.",
  "main": "dist/Miew.js",
  "module": "dist/Miew.module.js",
  "style": "dist/Miew.min.css",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epam/miew.git"
  },
  "license": "MIT",
  "scripts": {
    "ci": "run-s clean lint test-cover build",
    "ci-fast": "run-s lint:js test:unit build:demo",
    "lint": "run-s lint:*",
    "lint-fix": "run-s \"lint:* -- --fix\"",
    "lint:js": "eslint -f unix .*.js *.js src/ demo/ examples/ test/ tools/",
    "lint:css": "stylelint src/**/*.scss demo/**/*.scss",
    "test": "run-s test:unit",
    "test-cover": "cross-env NODE_ENV=coverage nyc mocha",
    "test:unit": "mocha",
    "test:e2e": "rimraf ./test/e2e/mismatch/*.png && mocha --opts ./test/mocha.e2e.opts",
    "start": "webpack-dev-server --mode development --open",
    "clean": "rimraf build/* coverage/*",
    "build": "run-s build:*",
    "build:js": "run-s build:js:*",
    "build:js:bundle": "rollup -c",
    "build:js:minify": "uglifyjs build/dist/Miew.js -o build/dist/Miew.min.js -mc drop_console=true --comments /copyright/i --source-map \"content='build/dist/Miew.js.map',url='Miew.min.js.map'\"",
    "build:css": "run-s build:css:*",
    "build:css:bundle": "postcss src/Miew.scss -o build/dist/Miew.css",
    "build:css:minify": "postcss build/dist/Miew.css -o build/dist/Miew.min.css --use cssnano --map",
    "build:demo": "webpack --mode production",
    "build:examples": "ncp examples/ build/examples/ && run-s build:examples:*",
    "build:examples:viewer": "mkdirp build/v && ncp examples/fullscreen.html build/v/index.html",
    "build:examples:webpack": "cd build/examples/ && webpack --mode production --config miew_via_webpack.config.js",
    "build:examples:browserify": "cd build/examples/ && browserify -e miew_via_browserify.js -o miew_via_browserify.bundle.js",
    "build:examples:react": "cd build/examples/ && webpack --mode production --config miew_via_react.config.js",
    "build:docs": "jsdoc -c jsdoc.config.js",
    "demo": "node tools/serve.js",
    "show:cover": "node tools/open.js coverage/lcov-report/index.html",
    "show:docs": "node tools/open.js build/docs/index.html",
    "show:e2e": "node tools/open.js test/e2e/mismatch/*.html",
    "dist": "ncp build/dist/ dist/ && git add -A dist",
    "preversion": "run-s lint test",
    "version": "cross-env NODE_ENV=production run-s build:js build:css dist",
    "jison": "node tools/jison.js src/utils/*.jison",
    "jison:one": "node ./tools/jison.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "bugs": {
    "url": "https://github.com/epam/miew/issues"
  },
  "homepage": "https://github.com/epam/miew#readme",
  "keywords": [
    "molecule",
    "protein",
    "chemistry",
    "webgl",
    "threejs"
  ],
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions",
    "IE >= 11",
    "last 2 iOS versions",
    "last 1 Android version",
    "last 2 ChromeAndroid versions",
    "last 2 FirefoxAndroid versions"
  ],
  "nyc": {
    "cache": true,
    "all": true,
    "include": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "exclude": [
      "@(node_modules|dist|build|vendor|demo|coverage|docs|examples|test|tools)",
      "**/*.test.js"
    ],
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "greenkeeper": {
    "ignore": [
      "bootstrap",
      "jquery.terminal"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-external-helpers": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/register": "^7.7.4",
    "@babel/runtime": "^7.7.4",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-preset-airbnb": "^4.4.0",
    "bootstrap": "^3.3.7",
    "bootstrap-switch": "=3.3.4",
    "browserify": "^16.5.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "copy-webpack-plugin": "^5.0.5",
    "coveralls": "^3.0.9",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.1",
    "cssnano": "^4.1.10",
    "dirty-chai": "^2.0.1",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.17.0",
    "express": "^4.17.1",
    "file-loader": "^5.0.2",
    "glob": "^7.1.6",
    "handlebars": "^4.5.3",
    "html-webpack-plugin": "^3.2.0",
    "jison": "^0.4.18",
    "jquery": "3.4.1",
    "jquery.terminal": "=2.1.0",
    "jsdoc": "^3.6.3",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^0.8.0",
    "mkdirp": "^0.5.1",
    "mocha": "^6.2.2",
    "ncp": "^2.0.0",
    "node-resemble-js": "^0.2.0",
    "npm-run-all": "^4.1.3",
    "nyc": "^14.1.1",
    "open": "^7.0.0",
    "postcss-advanced-variables": "^3.0.0",
    "postcss-calc": "^7.0.1",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-scss": "^2.0.0",
    "proxyquire": "^2.1.0",
    "raw-loader": "^4.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.8",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-string": "^3.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "sinon": "^8.0.0",
    "sinon-chai": "^3.2.0",
    "spin.js": "^4.1.0",
    "style-loader": "^1.0.1",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "three": "0.111.0",
    "toastr": "^2.1.4",
    "uglify-js": "^3.7.1",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "yargs": "^15.0.2"
  }
}
